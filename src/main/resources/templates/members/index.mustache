{{>layouts/header}}

<table class="table table-dark">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#memberList}}
        <tr class="table-active">
            <th>{{id}}</th>
            <td><a href="members/{{id}}">{{email}}</td>
            <td>{{password}}</td>
        </tr>
    {{/memberList}}
    </tbody>
</table>

<a href="/signup">New Member</a>
{{>layouts/footer}}